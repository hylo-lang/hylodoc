type Vector2 {
  /// First component of the vector.
  var x: Float64

  /// Second component of the vector.
  var y: Float64

  /// Initializes the vector with two components.
  init(x: Float64, y: Float64) {
    self.x = x
    self.y = y
  }

  /// Returns the vector multiplied by scale, without mutating the original vector.
  fun multiplied(by scale: Float64) -> Vector2 { 
    return Vector2(x: self.x * scale, y: self.y * scale)
   }

